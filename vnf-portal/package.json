{
  "name": "vnf-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "e2e": "npx playwright test",
    "e2e-ui": "npx playwright test --ui",
    "e2e-reports": "npx playwright show-report",
    "watch": "ng build --watch --configuration development",
    "start": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve",
    "build_dev": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "analyze_build": "ng build --stats-json",
    "analyze_report": "npm run build_stats-json & webpack-bundle-analyzer dist/fsd-fe/stats.json",
    "vitest": "vitest",
    "vitest-ui": "vitest --ui --coverage.enabled=true",
    "vitest-coverage": "vitest run --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/service-worker": "^17.3.0",
    "@ngneat/transloco": "^6.0.4",
    "@ngrx/store": "^17.1.1",
    "animate.css": "^4.1.1",
    "hotkeys-js": "^3.13.7",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "ngx-skeleton-loader": "^9.0.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@playwright/test": "^1.42.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.18",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.37",
    "prettier": "^3.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.4.2",
    "vitest": "^1.4.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
